function Replace(This, ToThis)
	_G.CurrentTextBox.Text = string.gsub(_G.CurrentTextBox.Text, This, ToThis)
end

local ReplaceTexts = {
	[" WaitForChild"] = "WaitForChild",
	["WaitForChild "] = "WaitForChild",
	[" WaitForChild "] = "WaitForChild",
	[" FindFirstChildOfClass"] = "FindFirstChildOfClass",
	["FindFirstChildOfClass "] = "FindFirstChildOfClass",
	[" FindFirstChildOfClass "] = "FindFirstChildOfClass",
	[" FindFirstChild"] = "FindFirstChild",
	["FindFirstChild "] = "FindFirstChild",
	[" FindFirstChild "] = "FindFirstChild",
	[" GetService"] = "GetService",
	["GetService "] = "GetService",
	[" GetService "] = "GetService",
	[" GetDescendants"] = "GetDescendants",
	["GetDescendants "] = "GetDescendants",
	[" GetDescendants "] = "GetDescendants",
	[" GetChildren"] = "GetChildren",
	["GetChildren "] = "GetChildren",
	[" GetChildren "] = "GetChildren",
	["game.Players"] = 'game:GetService("Players")',
	["game.Workspace"] = 'game:GetService("Workspace")',
	["workspace"] = 'game:GetService("Workspace")',
	["   "] = "	"
}

spawn(function()
	while wait() do
		for Wrong, Correct in pairs(ReplaceTexts) do
			pcall(function()
				if _G.CurrentTextBox.Text:find(Wrong) and not _G.CurrentTextBox:IsFocused() then
					Replace(Wrong, Correct)
				end
			end)
		end
	end
end)

function CheckScriptsInChr()
	warn("")
	warn("Start")
	warn("----------- [ Check Script(s) In Character ] ----------")
	print("")
	for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		if v:IsA("Script") or v:IsA("LocalScript") or v:IsA("BoolValue") then
			if v.ClassName == "LocalScript" then
				print("		  Class Name" .. " : " .. v.ClassName .. " | " .. "Name : " .. v.Name)
			elseif v.ClassName == "Script" then
				print("		  Class Name" .. " : " .. v.ClassName .. "	  | " .. "Name : " .. v.Name)
			elseif v.ClassName == "BoolValue" then
				print("		  Class Name" .. " : ".. v.ClassName .. " | " .. "Name : " .. v.Name .. " | Value = " .. tostring(v.Value))
			end
		end
	end
	print("")
	warn("----------- [ End Of Check Script(s) In Character ] ----------")
	warn("End")
	warn("")
end

function UpdateLines()
	spawn(function()
		pcall(function()
			if _G.Lines == true then
				s = _G.CurrentTextBox.Text
				local lin = 1
				s:gsub("\n", function()
					lin = lin + 1
				end)
				Lines.Text = ""
				for i = 1, lin do
					Lines.Text = Lines.Text .. i .. "\n"
				end
			elseif _G.Lines == false then
				Lines.Text = "1"
			end
		end)
	end)
end

function updateSize()
	spawn(function()
		pcall(function()
			if _G.CurrentTextBox:IsFocused() then
				_G.CurrentTextBox.Size = UDim2.new(0, 100000, 0, 100000)
				ScrollFrame.CanvasSize = UDim2.new(0, 100000, 0, 100000)
			elseif not _G.CurrentTextBox:IsFocused() then
				_G.CurrentTextBox.Size = UDim2.new(0, _G.CurrentTextBox.TextBounds.X + 200, 0, _G.CurrentTextBox.TextBounds.Y + 200)
				ScrollFrame.CanvasSize = UDim2.new(0, _G.CurrentTextBox.TextBounds.X + 50, 0, _G.CurrentTextBox.TextBounds.Y)
			end
		end)
	end)
end

updateSize()

function SmoothDrag(frame)
	local UIS = game:GetService('UserInputService')
	local dragToggle = nil
	local dragSpeed = 0
	local dragStart = nil
	local startPos = nil
	
	function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end

SmoothDrag(Frame)

spawn(function()
	while true do wait()
		UpdateLines()
		updateSize()
	end
end)

function getHie(obj)
	local fullname
	local period

	if string.find(obj.Name, ' ') or string.find(obj.Name, '1') or string.find(obj.Name, '2') or string.find(obj.Name, '3') or string.find(obj.Name, '4') or string.find(obj.Name, '5') or string.find(obj.Name, '6') or string.find(obj.Name, '7') or string.find(obj.Name, '8') or string.find(obj.Name, '9') or string.find(obj.Name, '0') then
		fullname = '["'..obj.Name..'"]'
		period = false
	else
		fullname = obj.Name
		period = true
	end

	local getS = obj
	local parent = obj
	local service = ''

	if getS.Parent ~= game then
		repeat
			getS = getS.Parent
			service = getS.ClassName
		until getS.Parent == game
	end

	if parent.Parent ~= getS then
		repeat
			parent = parent.Parent
			if string.find(tostring(parent),' ') then
				if period then
					fullname = '["'..parent.Name..'"].'..fullname
				else
					fullname = '["'..parent.Name..'"]'..fullname
				end
				period = false
			else
				if period then
					fullname = parent.Name..'.'..fullname
				else
					fullname = parent.Name..''..fullname
				end
				period = true
			end
		until parent.Parent == getS
	elseif string.find(tostring(parent),' ') then
		fullname = '["'..parent.Name..'"]'
		period = false
	end

	if period then
		return 'game:GetService("'..service..'").'..fullname
	else
		return 'game:GetService("'..service..'")'..fullname
	end
end

function CheckToolScripts()
	print("")
	warn("Start")
	warn("----------- [ Check Tool Script(s) ] ----------")
	print("")
	for i,t in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		if t:IsA("Tool") then
			print("		Tool Name :",t.Name)
			for i,v in pairs(t:GetDescendants()) do
				if v:IsA("Script") or v:IsA("LocalScript") or v:IsA("RemoteEvent") then
					if v.ClassName == "LocalScript" then
						print("		  Class Name".." : "..v.ClassName.." | ".."Name : "..v.Name)
					elseif v.ClassName == "Script" then
							print("		  Class Name".." : "..v.ClassName.."	  | ".."Name : "..v.Name)
					elseif v.ClassName == "RemoteEvent" then
						print("		  Class Name".." : "..v.ClassName.." | ".."Name : "..v.Name)
					end
				end
			end
		end
	end
	print("")
	warn("----------- [ End Of Check Tool Script(s) ] ----------")
	warn("End")
	print("")
end

function Executes(Num, Script)
	for i = 1, tonumber(Num) do
		spawn(Script)
	end
end

function calculate(expression)
	Num = string.gsub(expression, "÷", "/") 
	Num = string.gsub(Num, "×", "*")
	local result, error = loadstring("return " .. Num)
	if result then
		return result()
	else
		return error
	end
end

