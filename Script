BtnScript("Games", function()
	local LoadingScreenGui = Instance.new("ScreenGui")
	local LoadingFrame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local UICorner2 = Instance.new("UICorner")
	local UICorner3 = Instance.new("UICorner")
	local UICorner4 = Instance.new("UICorner")
	local UICorner5 = Instance.new("UICorner")
	local UICorner6 = Instance.new("UICorner")
	local ImageFrame = Instance.new("ImageLabel")
	local PlaceName = Instance.new("TextLabel")
	local PlaceId = Instance.new("TextLabel")
	local LoadScript = Instance.new("TextLabel")
	local LoadingFrame2 = Instance.new("TextLabel")
	local LoadingText = Instance.new("TextLabel")
	local LoadingColor = Instance.new("TextLabel")
	local Asset = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
	
	LoadingScreenGui.Name = "LoadingGui"
	LoadingScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	
	LoadingFrame.Name = "Frame"
	LoadingFrame.Parent = LoadingScreenGui
	LoadingFrame.Size = UDim2.new(0.277, 0, 0.4, 0)
	LoadingFrame.Position = UDim2.new(0.35, 0, 0.25, 0)
	LoadingFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	LoadingFrame.BackgroundTransparency = .2
	LoadingFrame.Draggable = true
	LoadingFrame.Active = true
	
	UICorner.Name = "UICorner"
	UICorner.Parent = LoadingFrame
	UICorner.CornerRadius = UDim.new(0, 10)
	
	PlaceName.Name = "PlaceName"
	PlaceName.Parent = LoadingFrame
	PlaceName.Size = UDim2.new(0, 280, 0, 30)
	PlaceName.Position = UDim2.new(0, 10, 0, 13)
	PlaceName.Text = "Checking.."
	PlaceName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	PlaceName.TextColor3 = Color3.fromRGB(0,0,0)
	PlaceName.TextSize = 20
	PlaceName.Visible = true
	PlaceName.TextScaled = true
	PlaceName.BackgroundTransparency = 0
	
	UICorner2.Name = "UICorner2"
	UICorner2.Parent = PlaceName
	UICorner2.CornerRadius = UDim.new(0, 7.5)
	
	PlaceId.Name = "PlaceId"
	PlaceId.Parent = LoadingFrame
	PlaceId.Size = UDim2.new(0, 280, 0, 30)
	PlaceId.Position = UDim2.new(0, 10, 0, 55)
	PlaceId.Text = "Checking."
	PlaceId.BackgroundColor3 = Color3.fromRGB(255,255,255)
	PlaceId.TextColor3 = Color3.fromRGB(0, 0, 0)
	PlaceId.TextSize = 20
	PlaceId.Visible = true
	PlaceId.TextScaled = true
	PlaceId.BackgroundTransparency = 0
	
	UICorner3.Name = "UICorner3"
