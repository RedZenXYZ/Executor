	Toggle.Parent = main
	Toggle.Name = "Toggle"
	Toggle.Size = UDim2.new(0, 100, 0, 40)
	Toggle.Position = UDim2.new(0.05, 0, 0.75, 0)
	Toggle.Text = "Hitbox (Off)"
	Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
	Toggle.TextScaled = true
	Toggle.TextSize = 20
	Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	Toggle.BackgroundTransparency = 0
	Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Toggle.BorderSizePixel = 1
	Toggle.Visible = true
	Toggle.MouseButton1Click:Connect(function()
		if Toggle.Text == "Hitbox (Off)" then
			Toggle.Text = "Hitbox (On)"
			Toggle.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			
			_G.Hitbox = true
			ToggleHitbox()
		elseif Toggle.Text == "Hitbox (On)" then
			Toggle.Text = "Hitbox (Off)"
			Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			
			_G.Hitbox = false
			wait()
			ToNormalHead()
			ToNormalHumanoidRootPart()
		end
	end)
	
	function ToggleHitbox()
		while _G.Hitbox == true do wait()
			CreateHitbox()
		end
	end
	
	function CreateHitbox()
		spawn(function()
			pcall(function()
				if HitPart.Text == "Head" then
					ToNormalHumanoidRootPart()
					for i,v in next, game:GetService('Players'):GetPlayers() do
						if v.Name ~= game:GetService('Players').LocalPlayer.Name and v.Character and v.Character:FindFirstChild("Head") then
							v.Character.Head.Size = Vector3.new(HitBox.Text,HitBox.Text,HitBox.Text)
							v.Character.Head.Transparency = TransparencyNum.Text
							v.Character.Head.Color = Color3.new(Red.Text,Green.Text,Blue.Text)
							v.Character.Head.Material = "Neon"
							v.Character.Head.CanCollide = false
						end
					end
				elseif HitPart.Text == "HumanoidRootPart" then
					ToNormalHead()
					for i,v in next, game:GetService('Players'):GetPlayers() do
						if v.Name ~= game:GetService('Players').LocalPlayer.Name and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
							v.Character.HumanoidRootPart.Size = Vector3.new(HitBox.Text,HitBox.Text,HitBox.Text)
							v.Character.HumanoidRootPart.Transparency = TransparencyNum.Text
							v.Character.HumanoidRootPart.Color = Color3.new(Red.Text,Green.Text,Blue.Text)
							v.Character.HumanoidRootPart.Material = "Neon"
							v.Character.HumanoidRootPart.CanCollide = false
						end
					end
				end
			end)
		end)
	end
	
	function ToNormalHead()
		spawn(function()
			pcall(function()
				for i,v in next, game:GetService('Players'):GetPlayers() do
					if v.Name ~= game:GetService('Players').LocalPlayer.Name and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
						v.Character.Head.Size = Vector3.new(1, 1, 1)
						v.Character.Head.Transparency = 0
						v.Character.Head.CanCollide = false
					end
				end
			end)
		end)
	end
	
	function ToNormalHumanoidRootPart()
		spawn(function()
			pcall(function()
				for i,v in next, game:GetService('Players'):GetPlayers() do
					if v.Name ~= game:GetService('Players').LocalPlayer.Name and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
						v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
						v.Character.HumanoidRootPart.Transparency = 1
						v.Character.HumanoidRootPart.CanCollide = false
					end
				end
			end)
		end)
	end
end)

