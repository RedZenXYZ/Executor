		MoveScrollingFrame.Visible = true
	else
		if MoveScrollingFrameButton.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			MoveScrollingFrameButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			MoveScrollingFrame.Visible = false
		end
