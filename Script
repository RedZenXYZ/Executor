----- [ FileS ] -----

	function MakeFolder(Name) local success, result = pcall(function() listfiles(Name) end) if not success then makefolder(Name) end end
	function WriteFile(Name, Data) local success, result = pcall(function() local file = isfile(Name) if file then else error("File doesn't exist") end end) if not success then writefile(Name, Data) end end
	function jsonencode(Table) return game:GetService("HttpService"):JSONEncode(Table) end
	function jsondecode(TableString) return game:GetService("HttpService"):JSONDecode(TableString) end

	MakeFolder("Red Executor")
	MakeFolder("Red Executor/Script Tabs")
	MakeFolder("Red Executor/Scripts Hub")
	MakeFolder("Red Executor/Setting")
	SettingS =
	{
		HideLines = false,
		MoveScrollButtons = false,
		Language = "English"
	}
	StringSetting = jsonencode(SettingS)
	WriteFile("Red Executor/Setting/Settings", StringSetting)
	WriteFile("Red Executor/Script Tabs/Tab1", "--- Tab : " .. "1" .. '\n--- local functions | Plrs | Plr | Chr | Ws | Bp | CGui | PlrGui\n--- Functions : CheckScriptsInChr() | CheckToolScripts () | Clip("Text") | calculate("stringNumbers") | getHie(Object) | Replace("This", "ToThis") | Fling("Target Name") | Executes(Num, Source) | Output(String or "Text")\n\n')

----- [ Languages ] -----

	Languages =
	{
		English =
		{
			Done = "Done",
			Error = "Error",
			Tab = "Tab ",
			SearchBar = "Search Bar",
			RUSure = "Are You Sure?",
			Execute = "Execute",
			Running = "Running",
			Copy = "Copy",
			Copying = "Copying",
			Clear = "Clear",
			RemoveGui = "Remove Gui",
			HideLines1 = "Hide Lines",
			HideLines2 = "Show Lines"
		},
		Thai =
		{
			Done = "เสร็จสิ้น",
			Error = "ผิดพลาด",
			Tab = "ช่องที่ ",
			SearchBar = "ช่อง ค้นหา",
			RUSure = "แน่ใจ?",
			Execute = "รันสคริปต์",
			Running = "สคริปต์กำลังทำงาน",
			Copy = "คัดลอก",
			Copying = "กำลังคัดลอก",
			Clear = "ลบ",
			RemoveGui = "ลบหน้าต่างโปร",
			HideLines1 = "ซ่อนเส้นเลข",
			HideLines2 = "แสดงเส้นเลข"
		}
	}
	
	Language = Languages.English
	
	function updateLang(LanguageFr)
		if LanguageFr == nil or LanguageFr == "" then
			CurLang = Languages.English
		elseif LanguageFr:lower() == "english" then
			CurLang = Languages.English
		elseif LanguageFr:lower() == "thai" then
			CurLang = Languages.Thai
		else
			CurLang = Languages.English
		end
		
		Language = CurLang
		
		Execute.Text = CurLang.Execute
		Copy.Text = CurLang.Copy
		Clear.Text = CurLang.Clear
		RemoveGui.Text = CurLang.RemoveGui
		SearchBar.PlaceholderText = CurLang.SearchBar
		
		if HideLines.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
			HideLines.Text = CurLang.HideLines1
		elseif HideLines.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			HideLines.Text = CurLang.HideLines2
		end
	end

