function BtnScript(Text, Source)
	local NewButton = Instance.new("TextButton")
	NewButton.Name = Text
	NewButton.Parent = ScrollFrame3
	NewButton.Size = UDim2.new(0, 90, 0, 30)
	NewButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	NewButton.Text = Text
	NewButton.TextScaled = true
	NewButton.TextSize = 15
	NewButton.MouseButton1Click:Connect(Source)
end

function SureBtn(Text, Source)
	local NewButton = Instance.new("TextButton")
	NewButton.Name = Text
	NewButton.Parent = ScrollFrame3
	NewButton.Size = UDim2.new(0, 90, 0, 30)
	NewButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	NewButton.Text = Text
	NewButton.TextScaled = true
	NewButton.TextSize = 15
	NewButton.MouseButton1Click:Connect(function()
		if NewButton.Text == Text then
			NewButton.Text = "U Sure?"
			NewButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			NewButton.TextColor3 = Color3.fromRGB(0, 0, 0)
			wait(1.5)
			NewButton.Text = Text
			NewButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			NewButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		else
			if NewButton.Text == "U Sure?" then
				NewButton.Text = Text
				NewButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				NewButton.TextColor3 = Color3.fromRGB(0, 0, 0)
				spawn(Source)
			end
		end
	end)
end

