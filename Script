function AddScript()
	for i, tab in pairs(ScriptsTabScrollingFrame:GetChildren()) do
		if tab:IsA("Frame") and tab:FindFirstChild("TabTextBox") and tab:FindFirstChild("TabTextBox").BackgroundColor3 == Color3.fromRGB(225, 0, 0) then
			tab:FindFirstChild("TabTextBox").BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			tab:FindFirstChild("TabTextBox").Active = false
		end
	end
	for i, source in pairs(ScrollFrame:GetChildren()) do
		if source:IsA("TextBox") then
			source.Visible = false
		end
	end
	
	local Tabs = {}
	local Num = 1
	for _, number in pairs(ScriptsTabScrollingFrame:GetChildren()) do
		if number:IsA("Frame") then
			table.insert(Tabs, tonumber(tostring(number.Name:gsub("%D+", ""))))
		end
	end
	
	repeat
		if table.find(Tabs, Num) then
			Num = Num + 1
		end
	until not table.find(Tabs, Num)
	
	local Num = tostring(Num)
	
	local ScriptTab = Instance.new("Frame", ScriptsTabScrollingFrame)
	local TabTextBox = Instance.new("TextBox", ScriptTab)
	local TabBtn = Instance.new("TextButton", ScriptTab)
	local Close = Instance.new("TextButton", ScriptTab)
	local Source = Instance.new("TextBox", ScrollFrame)
	_G.CurrentTextBox = Source

	ScriptTab.Name = Num .. "ScriptTab"
	ScriptTab.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	ScriptTab.Size = UDim2.new(0, 80, 0, 20)
	ScriptTab.BorderSizePixel = 0
	ScriptTab.Active = false

	TabBtn.Name = "TabBtn"
	TabBtn.Size = UDim2.new(0, 60, 0, 20)
	TabBtn.Text = ""
	TabBtn.Active = false
	TabBtn.BorderSizePixel = 0
	TabBtn.BackgroundTransparency = 1
	TabBtn.MouseButton1Click:Connect(function()
		_G.CurrentTextBox = Source
		for i, tab in pairs(ScriptsTabScrollingFrame:GetChildren()) do
			if tab:IsA("Frame") and tab:FindFirstChild("TabTextBox") and tab:FindFirstChild("TabTextBox").BackgroundColor3 == Color3.fromRGB(225, 0, 0) then
				tab:FindFirstChild("TabTextBox").BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				tab:FindFirstChild("TabTextBox").Active = false
			end
		end
		for i, source in pairs(ScrollFrame:GetChildren()) do
			if source:IsA("TextBox") then
				source.Visible = false
			end
		end
		TabTextBox.BackgroundColor3 = Color3.fromRGB(225, 0, 0)
		Source.Visible = true
		TabTextBox.Active = true
	end)
	
	TabTextBox.Name = "TabTextBox"
	TabTextBox.Size = UDim2.new(0, 60, 0, 20)
	TabTextBox.Text = Language.Tab .. Num
	TabTextBox.TextSize = 15
	TabTextBox.ClearTextOnFocus = false
	TabTextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
	TabTextBox.TextScaled = true
	TabTextBox.Active = true
	TabTextBox.BorderSizePixel = 0
	TabTextBox.BackgroundColor3 = Color3.fromRGB(225, 0, 0)
	
	Close.Name = "Close"
	Close.Size = UDim2.new(0, 20, 0, 20)
	Close.Position = UDim2.new(0, 60, 0, 0)
	Close.Text = "X"
	Close.TextColor3 = Color3.fromRGB(255, 255, 255)
	Close.TextScaled = true
	Close.Active = false
	Close.BorderSizePixel = 0
	Close.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Close.MouseButton1Click:Connect(function()
		if Close.Text == "X" then
			Close.Text = "S?"
			Close.TextColor3 = Color3.fromRGB(0, 0, 0)
			Close.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			wait(1)
			Close.TextColor3 = Color3.fromRGB(255, 255, 255)
			Close.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			Close.Text = "X"
		elseif Close.Text == "S?" then
			ScriptTab:Destroy()
			Source:Destroy()
		end
	end)
	
	Source.Name = "Source" .. Num
	Source.BackgroundTransparency = 1
	Source.Parent = ScrollFrame
	Source.Size = UDim2.new(0, 2020, 0, 10000)
	Source.Text = "--- " .. Language.Tab .. ": " .. Num .. '\n--- local functions | Plrs | Plr | Chr | Ws | Bp | CGui | PlrGui\n--- Functions : CheckScriptsInChr() | CheckToolScripts () | Clip("Text") | calculate("stringNumbers") | getHie(Object) | Replace("This", "ToThis") | Fling("Target Name") | Executes(Num, Source) | Output(String or "Text")\n\n'
	Source.PlaceholderText = "Paste Script Here"
	Source.PlaceholderColor3 = Color3.fromRGB(255, 0, 0)
	Source.TextScaled = false
	Source.TextSize = 15
	Source.TextXAlignment = Enum.TextXAlignment.Left
	Source.TextYAlignment = Enum.TextYAlignment.Top
	Source.ClearTextOnFocus = false
	Source.MultiLine = true
	Source.Visible = true
	Source.Font = Enum.Font.Code
	
	if _G.HideLines == false then
		Source.Position = UDim2.new(0, 35, 0, 0)
	elseif _G.HideLines == true then
		Source.Position = UDim2.new(0, 4, 0, 0)
	end
end

