RemoveGui.TextColor3 = Color3.fromRGB(0, 0, 0)
RemoveGui.TextScaled = true
RemoveGui.TextSize = 15
RemoveGui.MouseButton1Click:Connect(function()
	if RemoveGui.Text == Language.RemoveGui then
		RemoveGui.Text = Language.RUSure
		RemoveGui.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		wait(1.5)
		RemoveGui.Text = Language.RemoveGui
		RemoveGui.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	elseif RemoveGui.Text == Language.RUSure then
			Executor:Destroy()
			_G.Lines = false
	end
end)

